{
  "name": "lujo5/lumen-rest",
  "description": "Lumen package for creating simple REST API",
  "type": "library",
  "version": "1.0.0",
  "keywords": [
    "Lumen",
    "REST",
    "API"
  ],
  "require": {
    "php": "^7.1",
    "laravel/lumen-framework": "5.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "orchestra/testbench": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Lujo5\\Lumen\\Rest\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lujo5\\Lumen\\Rest\\Test": "tests"
    }
  },
  "authors": [
    {
      "name": "lujo5",
      "email": "lukamatosevic5@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "Lujo5\\Lumen\\Rest\\LumenRestServiceProvider"
      ]
    }
  }
}
